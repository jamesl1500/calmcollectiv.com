# Environment variables for CalmCollectiv Web Application

# Application
NEXT_PUBLIC_APP_NAME=CalmCollectiv
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Authentication
NEXT_PUBLIC_AUTH_PROVIDER=google

# OpenAI API Key (if applicable)
NEXT_PUBLIC_OPENAI_API_KEY=your-openai-api-key

# Analytics (if using Google Analytics)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Misc
NEXT_PUBLIC_ENV=development